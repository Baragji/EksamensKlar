    processFile(file) {
        const reader = new FileReader();
        
        reader.onload = (e) => {
            const content = {
                name: file.name,
                type: file.type,
                size: file.size,
                content: e.target.result,
                processed: false
            };
            
            this.userData.content.push(content);
            this.updateContentPreview();
        };
        
        if (file.type.startsWith('text/')) {
            reader.readAsText(file);
        } else {
            reader.readAsDataURL(file);
        }
    }

