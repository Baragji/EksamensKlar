// Advanced Onboarding System with AI Learning Path Generation
class OnboardingSystem {
    constructor() {
        this.currentStep = 0;
        this.steps = ['welcome', 'subject', 'content', 'timeline', 'generate'];
        this.userData = {
            subject: null,
            subjectEmoji: null,
